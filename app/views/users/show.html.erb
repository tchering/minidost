<% if @user == current_user && current_user.contractor? %>
  <%= render "contractor_dashboard",user: @user,active_tasks_applications_count: @active_tasks_applications_count,pending_applications: @pending_applications,task: @task %>
<% else %>
  <%= render "subcontractor_dashboard", user: @user,active_tasks_applications_count: @active_tasks_applications_count,task: @task %>
<% end %>
